<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Patient ID Sequence -->
        <record id="seq_clinic_patient" model="ir.sequence">
            <field name="name">Patient ID</field>
            <field name="code">clinic.patient</field>
            <field name="prefix">PAT</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Prescription Number Sequence -->
        <record id="seq_clinic_prescription" model="ir.sequence">
            <field name="name">Prescription Number</field>
            <field name="code">clinic.prescription</field>
            <field name="prefix">RX</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Vital Signs Record Sequence -->
        <record id="seq_clinic_vital_signs" model="ir.sequence">
            <field name="name">Vital Signs Record</field>
            <field name="code">clinic.vital.signs</field>
            <field name="prefix">VS</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

